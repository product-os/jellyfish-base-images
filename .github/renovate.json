{
  "extends": [
    "config:base",
    "docker:enableMajor"
  ],
  "enabledManagers": [
    "npm",
    "dockerfile"
  ],
  "pruneStaleBranches": false,
  "rangeStrategy": "bump",
  "commitMessagePrefix": "patch:",
  "commitBody": "Change-type: patch",
  "prHourlyLimit": 0,
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "groupName": "non-major",
      "updateTypes": [
        "minor",
        "patch"
      ],
      "automerge": true
    }
  ]
}
